from datetime import date
atual = date.today().year
maiores = 0
menores = 0

for i in range(1, 6):
  anonasc = int(input(f"Ano nascimento {i}Âª pessoa: "))

  idade = atual-anonasc

  if idade >= 18:
    maiores += 1

  elif idade < 18:
    menores += 1

print(f"Ano: {atual}")
print(f"Maiores de idade: {maiores} pessoas")
print(f"Menores de idade: {menores} pessoas")
