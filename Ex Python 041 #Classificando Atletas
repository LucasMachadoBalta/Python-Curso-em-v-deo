from datetime import date
anoatual = date.today().year

nasc = int(input("Ano nascimento: "))
idade = anoatual-nasc

print(f"IDADE: {idade}\nCATEGORIA: ", end='')
if idade<=9:
  print("MIRIM")

elif idade<=14:
  print("INFANTIL")

elif idade<=19:
  print("JUNIOR")

elif idade<=25:
  print("SÃŠNIOR")

elif idade>25:
  print("MASTER")

