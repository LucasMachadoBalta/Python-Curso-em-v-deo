l1 = float(input("Lado 1: "))
l2 = float(input("Lado 2: "))
l3 = float(input("Lado 3: "))

if l1<l2+l3 and l2<l1+l3 and l3<l2+l1:
  print(f"{l1}, {l2}, {l3} formam triângulo ", end='')
  if l1==l2==l3:
    print("equilátero")

  elif l1==l2!=l3 or l3==l1!=l2 or l2==l3!=l1:
    print("isóceles")

  elif l1!=l2!=l3!=l1:
    print("escaleno")

else:
  print(f"{l1}, {l2}, {l3} não formam triângulo")
